Proceso Comprobador_de_numeros_Positivos_Pero_Mejor
	
	Definir num Como Entero;
	
	Escribir 'Escriba cualquier número';
	Leer num;
	
	Borrar Pantalla;
	Si num > 0 Entonces
		Escribir 'Usted escribio un número positivo';
		
	SiNo
		Escribir "Usted escribio un número negativo";
		
	FinSi
FinProceso