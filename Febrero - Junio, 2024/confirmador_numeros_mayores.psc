Proceso confirmador_numeros_mayores
	
	Definir a, b, c Como Real;
	
	Escribir 'ingrese tres numeros';
	Leer a, b ,c;
	
	Si a>b y a>c Entonces
		Escribir 'El numero mayor es ', a;
		
	SiNo Si b>a y b>c Entonces 
			Escribir 'El numero mayor es ', b;
			
		SiNo Escribir 'hola';
		FinSi
	FinSi
	
	
	
Fin proceso
